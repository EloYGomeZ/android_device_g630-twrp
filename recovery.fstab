# mount point	fstype	device
	
/boot           emmc    /dev/block/platform/msm_sdcc.1/by-name/boot
/cache          ext4    /dev/block/platform/msm_sdcc.1/by-name/cache
/cust		    ext4	/dev/block/platform/msm_sdcc.1/by-name/cust	            flags=display="Cust";backup=1;wipeingui
/data           ext4    /dev/block/platform/msm_sdcc.1/by-name/userdata
/internal_sd    vfat    /dev/block/platform/msm_sdcc.1/by-name/grow             flags=display="Internal Storage";storage;wipeingui
/system         ext4    /dev/block/platform/msm_sdcc.1/by-name/system
/sdcard         vfat    /dev/block/mmcblk1p1    /dev/block/mmcblk1              flags=display="Micro SDcard";storage;wipeingui;removable
